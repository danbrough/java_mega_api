<configuration>

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %highlight(&lt;%level&gt; %logger{10} - %msg%n)</pattern>
		</encoder>
	</appender>



	<root level="trace">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>

